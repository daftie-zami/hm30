
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'platform/ar934x_wmac'
	option htmode 'HT20'
	option country '00'
	option txpower '27'
	option distance '40000'
	option channel '112'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option key 'ZJXtt8LHKrgLw48TsX1Su'
	option encryption 'sae'
	option ifname 'wlan0'
	option mode 'mesh'
	option disassoc_low_ack '0'
	option mesh_fwding '1'
	option mesh_rssi_threshold '0'
	option macaddr '80:c2:ee:e1:17:9f'
	option mesh_id 'ab6ec363'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ehci-platform/usb1/1-1/1-1:1.0'
	option htmode 'HT20'
	option legacy_rates '0'
	option channel '8'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ifname 'wlan_usb'
	option wmm '0'
	option key '12345678'
	option encryption 'psk'
	option macaddr '80:8b:a1:56:28:a0'
	option ssid 'SIYI-6A02203700'

